{
  "requests":[
     {
        "verb":"GET",
        "path":"/api/inference_modules",
        "validation":{
           "jsonArray":{
              "minCount":1,
              "forEach":[
                 {
                    "jsonObject":[
                       {
                          "field":"id"
                       },
                       {
                          "field":"device"
                       },
                       {
                          "field":"upload_status"
                       },
                       {
                          "field":"recommended_fps"
                       },
                       {
                          "field":"name"
                       },
                       {
                          "field":"url"
                       }
                    ]
                 }
              ]
           }
        }
     }
  ]
}
